{
  "name": "doccollab",
  "version": "1.0.0",
  "description": "Real-time collaborative document editor",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"cd server && npm run dev\" \"cd client && npm run dev\"",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "seed": "cd server && npm run seed",
    "check": "node check-app.js"
  },
  "keywords": [
    "document",
    "editor",
    "collaboration",
    "real-time"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}
